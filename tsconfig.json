{
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./buildcache/build_info",
    "baseUrl": ".",
    "outDir": "dist",
    "module": "esnext",
    "target": "es6",
    "lib": ["es7", "dom"],
    "sourceMap": true,
    "newLine": "LF",
    "allowJs": false,
    "moduleResolution": "node",
    "rootDir": "src",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "noImplicitAny": true,
    "importHelpers": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "strict": true,
    "types": [],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    "build",
    "dist",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "src/setupTests.ts",
    "Gulpfile.js"
  ],
  "include": ["src"]
}
